package gg;

import java.util.Scanner;

public class ggclass {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int[] nums = new int[5];
		for (int i = 0; i < nums.length; i++) 
		{
			nums[i] = in.nextInt();
		}

		for (int i = 0; i < nums.length; i++) 
		{
			System.out.print(nums[i]);
		}
		for (int i = nums.length; i >0; i--) 
		{
			System.out.print(nums[i]);		
		}
		
	}


	}

